<section class="admin-products-index">

  <header class="page-header">
    <h1>Admin &raquo; All Categories</h1>
  </header>

<div class="container">
  <h1>Categories</h1>
  <%= link_to 'New Category', [:new, :admin, :category], class: 'btn btn-info' %>
  <table class="table">
    <thead>
      <tr>
        <th>Name</th>
      </tr>
    </thead>
    <tbody>
      <% @categories.each do |category| %>
        <tr>
          <td><%= category.name %></td>
         
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

</section>

